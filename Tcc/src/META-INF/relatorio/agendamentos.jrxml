	<queryString language="SQL">
		<![CDATA[SELECT
     agendamentos.`ID` AS agendamentos_ID,
     agendamentos.`OBSERVACAO` AS agendamentos_OBSERVACAO,
     agendamentos.`DATAHORA` AS agendamentos_DATAHORA,
     agendamentos.`REALIZADO` AS agendamentos_REALIZADO,
     agendamentos.`CLIENTE_ID` AS agendamentos_CLIENTE_ID,
     agendamentos.`FUNCIONARIO_ID` AS agendamentos_FUNCIONARIO_ID,
     agendamentos_servicos.`Agendamento_ID` AS agendamentos_servicos_Agendamento_ID,
     agendamentos_servicos.`servicos_ID` AS agendamentos_servicos_servicos_ID,
     servicos.`ID` AS servicos_ID,
     servicos.`ATIVO` AS servicos_ATIVO,
     servicos.`COMISSAO` AS servicos_COMISSAO,
     servicos.`DESCICAO` AS servicos_DESCICAO,
     servicos.`DESCONTOMAXIMO` AS servicos_DESCONTOMAXIMO,
     servicos.`DURACAOAPROXIMADA` AS servicos_DURACAOAPROXIMADA,
     servicos.`NOME` AS servicos_NOME,
     servicos.`VALOR` AS servicos_VALOR,
     clientes.`ID` AS clientes_ID,
     clientes.`ATIVO` AS clientes_ATIVO,
     clientes.`DATACADASTRO` AS clientes_DATACADASTRO,
     pessoas.`ID` AS pessoas_ID,
     pessoas.`DTYPE` AS pessoas_DTYPE,
     pessoas.`CELULAR` AS pessoas_CELULAR,
     pessoas.`CPF` AS pessoas_CPF,
     pessoas.`DATANASCIMENTO` AS pessoas_DATANASCIMENTO,
     pessoas.`ENDERECOBAIRRO` AS pessoas_ENDERECOBAIRRO,
     pessoas.`ENDERECOCEP` AS pessoas_ENDERECOCEP,
     pessoas.`ENDERECOCOMPLEMENTO` AS pessoas_ENDERECOCOMPLEMENTO,
     pessoas.`ENDERECONUMERO` AS pessoas_ENDERECONUMERO,
     pessoas.`ENDERECORUA` AS pessoas_ENDERECORUA,
     pessoas.`NOME` AS pessoas_NOME,
     pessoas.`OBSERVACAO` AS pessoas_OBSERVACAO,
     pessoas.`RG` AS pessoas_RG,
     pessoas.`SEXO` AS pessoas_SEXO,
     pessoas.`TELEFONE` AS pessoas_TELEFONE,
     pessoas.`ENDERECOCIDADE_ID` AS pessoas_ENDERECOCIDADE_ID
FROM
     `agendamentos` agendamentos INNER JOIN `agendamentos_servicos` agendamentos_servicos ON agendamentos.`ID` = agendamentos_servicos.`Agendamento_ID`
     INNER JOIN `servicos` servicos ON agendamentos_servicos.`servicos_ID` = servicos.`ID`
     INNER JOIN `clientes` clientes ON agendamentos.`CLIENTE_ID` = clientes.`ID`
     INNER JOIN `pessoas` pessoas ON agendamentos.`CLIENTE_ID` = pessoas.`ID`
     AND pessoas.`ID` = agendamentos.`FUNCIONARIO_ID`
     AND pessoas.`ID` = clientes.`ID`]]>
	</queryString>
